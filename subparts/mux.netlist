INPUT a
OUTPUT b
VAR a:2, _l_1:2, _l_2:2, _l_3:2, _l_4:2, _a_slc_0_0, cas1:2, _l_7, cas2:2, _a_sel_1, b:2
IN
_l_1 = 00
_l_2 = 01
_l_3 = 10
_l_4 = 11
_a_slc_0_0 = SLICE 0 0 a
cas1 = MUX _a_slc_0_0 _l_1 _l_2
_l_7 = SLICE 0 0 a
cas2 = MUX _l_7 _l_3 _l_4
_a_sel_1 = SELECT 1 a
b = MUX _a_sel_1 cas1 cas2
